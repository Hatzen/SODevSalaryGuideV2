<hazelcast
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="" xmlns="http://www.hazelcast.com/schema/config">
    <cache name="buckets">
        <management-enabled>true</management-enabled>
    </cache>
    <cache name="filterConfigCache">
        <management-enabled>true</management-enabled>
    </cache>

    <network>
        <join>
            <tcp-ip enabled="true">
                <interface>127.0.0.1</interface>
            </tcp-ip>
        </join>
    </network>
</hazelcast>